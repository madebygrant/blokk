{
  "version": 2,
  "styles": {
    "color": {
      "text": "var(--wp--preset--color--black)",
      "background": "#fff"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--base)",
      "fontSize": "var(--wp--preset--font-size--small)",
      "lineHeight": "var(--wp--custom--line-height--body)",
      "fontWeight": "var(--wp--custom--font-weight--regular)"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--max-60)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--max-48)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--max-36)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        }
      }
    },
    "blocks": {
      "core/post-title": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--max-60)"
        }
      },
      "core/heading": {
        "typography": {
          "lineHeight": "var(--wp--custom--line-height--heading)"
        }
      }
    }
  },
  "settings": {
    "custom": {
      "fontWeight": {
        "regular": 400,
        "bold": 700
      },
      "lineHeight": {
        "one": 1,
        "heading": 1.1,
        "medium": 1.5,
        "body": 1.75
      },
      "spacing": {
        "tiny": ".5rem",
        "small": "1rem",
        "medium": "2rem",
        "large": "3rem",
        "outer": "6rem"
      },
      "width": {
        "tiny": "360px",
        "small": "480px",
        "medium": "640px",
        "medium-large": "768px",
        "large": "1024px",
        "xlarge": "1280px",
        "xxlarge": "1920px",
        "full": "100%",
        "screen": "100vw",
        "min": "min-content",
        "max": "max-content",
        "content": "1280px",
        "site": "1920px"
      }
    },
    "border": {
      "customRadius": false
    },
    "color": {
      "defaultDuotone": false,
      "defaultPalette": false,
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "palette": [
        {
          "name": "White",
          "slug": "white",
          "color": "#ffffff"
        },
        {
          "name": "Black",
          "slug": "black",
          "color": "#000000"
        }
      ],
      "duotone": []
    },
    "layout": {
      "contentSize": "var(--wp--custom--width--content)",
      "wideSize": "1600px"
    },
    "spacing": {
      "customMargin": false,
      "customPadding": false,
      "units": [
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    },
    "typography": {
      "dropCap": false,
      "customFontSize": false,
      "customLineHeight": false,
      "fontSizes": [
        {
          "name": "16px",
          "slug": "small",
          "size": "16px"
        },
        {
          "name": "20px",
          "slug": "medium",
          "size": "20px"
        },
        {
          "name": "24px",
          "slug": "large",
          "size": "24px"
        },
        {
          "name": "36px",
          "slug": "max-36",
          "size": "clamp(24px, 3vw, 36px)"
        },
        {
          "name": "48px",
          "slug": "max-48",
          "size": "clamp(30px, 4vw, 48px)"
        },
        {
          "name": "60px",
          "slug": "max-60",
          "size": "clamp(36px, 5vw, 60px)"
        },
        {
          "name": "72px",
          "slug": "max-72",
          "size": "clamp(48px, 6vw, 72px)"
        }
      ],
      "fontFamilies": [
        {
          "name": "Work Sans",
          "fontFamily": "\"Work Sans\", sans-serif",
          "slug": "base"
        },
        {
          "name": "System Font",
          "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
          "slug": "system-font"
        },
        {
          "name": "Monospace",
          "fontFamily": "Consolas, Menlo, Monaco, \"Courier New\", monospace",
          "slug": "monospace"
        }
      ]
    }
  },
  "customTemplates": [
    {
      "name": "page-front",
      "title": "Front Page",
      "postTypes": [
        "page"
      ]
    },
    {
      "name": "page-no-title",
      "title": "Page - No Title",
      "postTypes": [
        "page",
        "post"
      ]
    }
  ],
  "templateParts": [
    {
      "name": "header",
      "title": "Header: With the logo only",
      "area": "header"
    },
    {
      "name": "header-title-only",
      "title": "Header: With the title only",
      "area": "header"
    },
    {
      "name": "header-logo-title-tagline",
      "title": "Header: With logo, title and tagline",
      "area": "header"
    },
    {
      "name": "header-search",
      "title": "Header: With logo, title, tagline and search bar",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ]
}